{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "ESNext.String"
    ],
    "allowJs": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "./compile",
    "strict": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
  },
  "include": [
    "./electron"
  ]
}
